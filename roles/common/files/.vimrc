execute pathogen#infect()
syntax on
filetype plugin indent on
set ts=4
set tabstop=4
set expandtab
set softtabstop=4
set shiftwidth=4
set number
set smartindent
set title
set whichwrap+=<>[]hl
set laststatus=2
set cursorline
set background=dark
set incsearch
set hlsearch
let base16colorspace=256
colorscheme base16-ocean
let g:rehash256 = 1
set backspace=indent,eol,start
command R .w !bash
